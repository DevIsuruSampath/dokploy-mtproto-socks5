# MTProto host port (container listens on 443). Use 8443 by default to avoid conflicts.
MTPROTO_PORT=8443

# SOCKS5 host port (container listens on 1080).
SOCKS5_PORT=1080

# SOCKS5 username for client authentication.
SOCKS5_USER=change_me

# SOCKS5 password (use a long, random value).
SOCKS5_PASS=change_me_to_a_long_random_password
